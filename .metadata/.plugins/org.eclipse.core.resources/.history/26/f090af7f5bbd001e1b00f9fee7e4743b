package application;

public class CelsiusToFahrenheit {

	public static void main(String[] args) {
		
		System.out.println("Exercício 4: Fatorial de um Número");
		Integer x = 3;
		Integer mult = 1;
		
		for(int i = x-1; i>=1; i--) {
			System.out.println("Valor atual de mult: "+ mult);
			System.out.println("Valor atual de i: "+i);
			mult *= i;
			System.out.println(mult);
		}
	}
}
