package application;

public class PrimeNumber {

	public static void main(String[] args) {
		System.out.println("Exercício 6: Verificar Número Primo");
		Integer x = 8;
		Integer divisions = 0;
			for(int i = 1; i<=x; i++) {
				if(x % i == 0 && x % x == 0){
					divisions++;
					System.out.println(i + " is a prime number " + divisions);
				}
			}
		}
}
